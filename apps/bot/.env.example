# Bot Configuration
BOT_TOKEN=your_discord_bot_token_here
BOT_PUBLIC_KEY=your_discord_public_key_here

# Environment
NODE_ENV=development
LOG_LEVEL=info

# Upload commands on startup (set to true for first run or when commands change)
UPLOAD_COMMANDS=false

# API Integration
API_URL=http://localhost:3000
API_SECRET=your_api_secret_here

# Database (optional)
DATABASE_URL=postgresql://user:password@localhost:5432/pawsitive
